<form action="/signup" name="signupForm" role="form">
	<div class="form-group">
		<input type="fullname" class="form-control"  id="fullname" placeholder="<%= t('User.fullname')%>">
	</div>
	<div class="form-group col-md-12">
		<input type="email" class="form-control"  id="email" placeholder="<%= t('User.email')%>">
	</div>
	<div class="form-group">
		<input type="password" class="form-control"  id="password" placeholder="<%= t('User.password')%>">
	</div>
	<div class="form-group col-md-4">
		<button type="submit" class="btn btn-warning" ><%= t('login.submit')%></button>
	</div>
</form>